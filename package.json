{
  "name": "@r-squared/rsquared-ees",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Trafterian",
  "license": "MIT",
  "private": true,
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "cli:create_in_internal_blockchain": "ts-node -r tsconfig-paths/register src/cli/create_in_internal_blockchain.ts",
    "monitor:ethereum_transactions": "ts-node -r tsconfig-paths/register cli.ts monitor-ethereum-transactions",
    "worker:create_in_internal_blockchain": "ts-node -r tsconfig-paths/register src/scripts/worker_create_in_internal_blockchain.ts",
    "worker:redeem_in_external_blockchain": "ts-node -r tsconfig-paths/register src/scripts/worker_redeem_in_external_blockchain.ts",
    "monitor:deposit_internal_contract_created": "ts-node -r tsconfig-paths/register src/scripts/monitor_deposit_internal_contract_created.ts",
    "monitor:deposit_internal_contract_refunded": "ts-node -r tsconfig-paths/register cli.ts monitor-deposit-internal-contract-refunded",
    "monitor:deposit_internal_contract_redeemed": "ts-node -r tsconfig-paths/register src/scripts/monitor_deposit_internal_contract_redeemed.ts",
    "monitor:deposit_internal_contract_burned": "ts-node -r tsconfig-paths/register cli.ts monitor-deposit-internal-contract-burned",
    "worker:external_contract_redeem": "ts-node -r tsconfig-paths/register cli.ts worker-external-contract-redeem",
    "monitor:withdraw_internal_contract_created": "ts-node -r tsconfig-paths/register cli.ts monitor-withdraw-internal-contract-created",
    "found:withdraw_internal_contract_creation": "ts-node -r tsconfig-paths/register cli.ts found-withdraw-internal-contract-creation",
    "worker:create_withdrawal_external_contract": "ts-node -r tsconfig-paths/register cli.ts worker-create-withdrawal-external-contract",
    "worker:withdraw_external_contract_redeemed": "ts-node -r tsconfig-paths/register cli.ts worker-withdraw-external-contract-redeemed",
    "execute:withdraw_internal_contract_redeem": "ts-node -r tsconfig-paths/register cli.ts execute-withdraw-internal-contract-redeem",
    "monitor:withdraw_internal_contract_redeem": "ts-node -r tsconfig-paths/register cli.ts monitor-withdraw-internal-contract-redeem",
    "monitor:withdraw_internal_contract_redeem_processed": "ts-node -r tsconfig-paths/register cli.ts monitor-withdraw-internal-contract-redeem-processed",
    "monitor:external_withdraw_contract_timelock": "ts-node -r tsconfig-paths/register cli.ts monitor-external-withdraw-contract-timelock",
    "update:sanctioned-addresses": "ts-node -r tsconfig-paths/register cli.ts update-sanctioned-addresses",
    "get-fee": "ts-node -r tsconfig-paths/register cli.ts get-fee",
    "set-fee": "ts-node -r tsconfig-paths/register cli.ts set-fee",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "test": "NODE_ENV=test mocha -r ts-node/register -r tsconfig-paths/register -r ./tests/index.ts ./tests/Context --recursive --extension ts",
    "test:integration": "NODE_ENV=test mocha -r ts-node/register -r tsconfig-paths/register -r ./tests/index.ts -r ./tests/Integration/hooks.ts ./tests/Integration --recursive --extension ts"
},
  "dependencies": {
    "@nestjs/cli": "^9.1.1",
    "@nestjs/common": "^9.0.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.7",
    "@nestjs/platform-express": "^9.0.7",
    "@nestjs/typeorm": "^9.0.0",
    "@r-squared/rsquared-js": "^6.0.7",
    "amqplib": "^0.10.3",
    "axios": "^1.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.5",
    "mysql2": "^2.3.3",
    "nest-commander": "^3.6.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "shallow-equal-object": "^1.1.1",
    "source-map-support": "^0.5.21",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2",
    "web3": "^1.7.4",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.20.7",
    "@types/amqplib": "^0.10.1",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/uuid": "^8.3.4",
    "@types/yargs": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "mocha": "^10.0.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  }
}
